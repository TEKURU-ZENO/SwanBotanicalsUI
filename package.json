{
  "name": "swan-botanicals",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Luxury botanical skincare website for Swan Botanicals",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.400.0",
    "recharts": "^2.8.0",
    "react-slick": "^0.29.0",
    "react-responsive-masonry": "^2.1.7",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-hook-form": "7.55.0",
    "sonner": "2.0.3",
    "re-resizable": "^6.9.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "terser": "^5.19.4",
    "@tailwindcss/vite": "^4.0.0-alpha.15",
    "@tailwindcss/postcss": "^4.0.0-alpha.15",
    "tailwindcss": "^4.0.0-alpha.15",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9"
  },
  "keywords": [
    "skincare",
    "botanical",
    "luxury",
    "react",
    "typescript",
    "tailwind",
    "framer-motion",
    "vite"
  ],
  "author": "Swan Botanicals",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
